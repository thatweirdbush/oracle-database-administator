<Window x:Class="oracle_database_administator.User.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:oracle_database_administator.User"
        mc:Ignorable="d"
        Title="PasswordWindow" Height="300" Width="400">
    <Grid>
        <PasswordBox x:Name="PasswordBox" HorizontalAlignment="Center" VerticalAlignment="Center" Width="120"/>
        <Button x:Name="OKButton" Content="Button" HorizontalAlignment="Left" Margin="345,256,0,0" VerticalAlignment="Top" Click="OKButton_Click"/>
        <Button x:Name="CancelButton" Content="Button" HorizontalAlignment="Left" Margin="299,256,0,0" VerticalAlignment="Top" Click="CancelButton_Click"/>

    </Grid>
</Window>
